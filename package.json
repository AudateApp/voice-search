{
  "name": "audate-voice-search",
  "version": "23.10.8",
  "description": "Bringing voice-forward experiences to the browser",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "genLogo": "gulp",
    "start:chrome": "gulp ChromeDemo",
    "start:firefox": "web-ext run --source-dir ./extension/",
    "package": "ng build --configuration=development && webpack --config webpack.config.js --env development && zip -r dist/audate-dev.zip dist/audate",
    "package-prod": "ng build --configuration=production && webpack --config webpack.config.js --env production && zip -r dist/audate-prod.zip dist/audate",
    "prettier": "prettier --write \"src/**/*.{js,json,css,scss,less,md,ts,html}\""
  },
  "private": true,
  "author": "Justice Ogbonna",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/justiceo/audate"
  },
  "keywords": [
    "chrome",
    "firefox",
    "addon",
    "extension",
    "voice",
    "search",
    "recording",
    "notes"
  ],
  "dependencies": {
    "@angular/animations": "^16.2.8",
    "@angular/cdk": "^16.2.7",
    "@angular/common": "^16.2.8",
    "@angular/compiler": "^16.2.8",
    "@angular/core": "^16.2.8",
    "@angular/elements": "^16.2.8",
    "@angular/forms": "^16.2.8",
    "@angular/platform-browser": "^16.2.8",
    "@angular/platform-browser-dynamic": "^16.2.8",
    "@angular/router": "^16.2.8",
    "@floating-ui/dom": "^1.5.3",
    "@sentry/angular-ivy": "^7.73.0",
    "@sentry/browser": "^7.73.0",
    "@sentry/tracing": "^7.73.0",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primeng": "^16.4.1",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2"
  },
  "peerDependencies": {
    "primeicons": "^5.0.0",
    "primeng": "^14.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.5",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
