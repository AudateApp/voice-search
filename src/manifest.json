{
  "name": "Audate",
  "description": "Bringing voice-forward experiences to the browser",
  "homepage_url": "https://github.com/justiceo/audate",
  "version": "22.04.03",
  "manifest_version": 3,
  "permissions": ["contextMenus", "storage", "management"],
  "host_permissions": ["*://*/*"],
  "action": {
    "default_icon": {
      "16": "assets/images/icon-16x16.png",
      "24": "assets/images/icon-24x24.png",
      "32": "assets/images/icon-32x32.png"
    },
    "default_title": "Audate",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "assets/images/icon-16x16.png",
    "32": "assets/images/icon-32x32.png",
    "48": "assets/images/icon-48x48.png",
    "128": "assets/images/icon-128x128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["index.html"],
      "matches": ["<all_urls>"]
    }
  ],
  "options_page": "options.html"
}
